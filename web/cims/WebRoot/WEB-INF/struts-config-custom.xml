<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="custInfoActionForm" type="com.ylink.cim.cust.view.CustInfoActionForm"></form-bean>
	</form-beans>
	
	<action-mappings>
		<action path="/custInfoAction"
			name="custInfoActionForm"
			attribute="custInfoActionForm"
			type="com.ylink.cim.cust.view.CustInfoAction"
			parameter="action"
			scope="request">
		</action>
		<action path="/custMngAction"
				name="custInfoActionForm"
				type="com.ylink.cim.cust.view.CustMngAction"
				parameter="action" 
				scope="request">
		</action>
	</action-mappings>
</struts-config>