<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
	<form-beans>
		<form-bean name="noticeMsgActionForm"
			type="com.ylink.cim.busioper.view.NoticeMsgActionForm"></form-bean>
		<form-bean name="pushMngActionForm"
			type="com.ylink.cim.busioper.view.PushMngActionForm"></form-bean>
	</form-beans>
	
	<action-mappings>
		<action path="/noticeMsgAction" name="noticeMsgActionForm"
			attribute="noticeMsgActionForm" type="com.ylink.cim.busioper.view.NoticeMsgAction"
			parameter="action" scope="request">
		</action>
		<action path="/pushMngAction" name="pushMngActionForm"
			attribute="pushMngActionForm" type="com.ylink.cim.busioper.view.PushMngAction"
			parameter="action" scope="request">
		</action>
	</action-mappings>
</struts-config>