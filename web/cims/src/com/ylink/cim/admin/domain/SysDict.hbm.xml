<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ylink.cim.admin.domain.SysDict" table="sys_dict">
        <composite-id name="id" class="com.ylink.cim.admin.domain.SysDictId">
        	<key-property name="dictType" column="dict_type"></key-property>
        	<key-property name="dictValue" column="dict_value"></key-property>
        </composite-id>
        
        <property name="dictName" column="dict_name"></property>
        <property name="display" column="display"></property>
        <property name="remark" column="remark"></property>
    </class>
</hibernate-mapping>
