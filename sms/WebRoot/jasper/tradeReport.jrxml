		<![CDATA[SELECT

     case inout_type when '0' then '收入' when '1' then '支出' else '' end as inout_type_name,
	remark, amount
    FROM
     `account_journal` account_journal
WHERE
     1=1 and trade_type not in ('17') and create_date  >= $P{beginDate} and create_date < $P{endDate}
	order by inout_type]]>
