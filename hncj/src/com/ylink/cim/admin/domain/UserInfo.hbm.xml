<?xml version="1.0" encoding='GB2312'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="com.ylink.cim.admin.domain.UserInfo" table="USER_INFO" >
        <id name="userId" column="USER_ID" type="java.lang.String">
            <generator class="assigned" />
        </id>
        <property name="userName" column="USER_NAME"  />
        <property name="userType" column="USER_TYPE" />
  		<property name="loginId" column="LOGIN_ID" />
        <property name="loginPwd" column="LOGIN_PWD" />
        <property name="createTime" column="CREATE_TIME"/>
        <property name="updateTime" column="UPDATE_TIME" />
		<property name="updateUser" column="UPDATE_USER" />
		<property name="pwdErrorTimes" column="PWD_ERROR_TIMES" />
		<property name="errorTime" column="ERROR_TIME" />
		<property name="branchNo" column="BRANCH_NO" />
    </class>
    
</hibernate-mapping>
